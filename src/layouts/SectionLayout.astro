---
import type { HTMLAttributes } from "astro/types";

interface Props extends HTMLAttributes<"section"> {}

const { class: extraClass = "", ...props } = Astro.props as Props;
---

<section class={`py-20  ${extraClass}`} {...props}>
  <div class="container mx-auto px-5 xl:px-15">
    <slot />
  </div>
</section>
